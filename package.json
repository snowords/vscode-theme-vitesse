{
  "name": "theme-snoword",
  "displayName": "Snoword Theme",
  "version": "0.4.4",
  "description": "Snoword theme for VS Code",
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "light",
    "dark"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/snowords/vscode-theme-vitesse"
  },
  "files": [
    "themes"
  ],
  "bugs": {
    "url": "https://github.com/snowords/vscode-theme-vitesse/issues"
  },
  "publisher": "Snoword",
  "scripts": {
    "build": "esno src/index.ts",
    "dev": "nodemon --watch src -e ts --exec \"esno src/index.ts\"",
    "vscode:prepublish": "yarn build",
    "lint": "eslint .",
    "release": "yarn build && npx bumpp --commit --tag --push --all && vsce publish --no-dependencies"
  },
  "contributes": {
    "themes": [
      {
        "label": "Snoword Light",
        "uiTheme": "vs",
        "path": "./themes/snoword-light.json"
      },
      {
        "label": "Snoword Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/snoword-dark.json"
      },
      {
        "label": "Snoword Light Soft",
        "uiTheme": "vs",
        "path": "./themes/snoword-light-soft.json"
      },
      {
        "label": "Snoword Dark Soft",
        "uiTheme": "vs-dark",
        "path": "./themes/snoword-dark-soft.json"
      }
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.19.4",
    "@antfu/utils": "^0.5.0",
    "@types/color": "^3.0.3",
    "@types/node": "^17.0.23",
    "color": "^4.2.3",
    "eslint": "^8.12.0",
    "esno": "^0.14.1",
    "nodemon": "^2.0.15",
    "tsup": "^5.12.4",
    "typescript": "^4.6.3",
    "vsce": "^2.7.0"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "icon": "icon.png",
  "preview": true
}
